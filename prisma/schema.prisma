generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  instructor
  student
  admin
}

model User {
  user_id    Int    @id @default(autoincrement())
  first_name String
  last_name  String
  email      String @unique
  password   String
  role       Role   @default(student)
}
